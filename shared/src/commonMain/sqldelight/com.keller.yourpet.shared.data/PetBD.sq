CREATE TABLE PetBD (
    id INTEGER NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    imageUrl TEXT NOT NULL,
    gender TEXT NOT NULL
);

selectAllPets:
SELECT * FROM PetBD;

selectFilteredPets:
SELECT * FROM PetBD
WHERE gender IN ?;

insertPet:
INSERT INTO PetBD(id, name, imageUrl, gender)
VALUES(?, ?, ?, ?);

deletePet:
DELETE FROM PetBD WHERE id = ?;

deletePets:
DELETE FROM PetBD;
